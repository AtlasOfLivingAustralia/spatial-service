{
  "name": "GDM Step 2",
  "description": "GDM Step 2. Defaults are set by GDM Step 1 for; cutpoint, distance ",
  "isBackground": false,
  "version": 1.0,
  "private": {
    "unique": false,
    "public": true
  },
  "input": {
    "biocacheServiceUrl": {
      "description": "biocacheServiceUrl",
      "type": "auto"
    },
    "layersServiceUrl": {
      "description": "layersServiceUrl",
      "type": "auto"
    },
    "process": {
      "description": "Prior GDM Step 1 process ID.",
      "type": "process",
      "constraints": {
        "name": "GDMStep1"
      }
    },
    "cutpoint": {
      "description": "The minimum number of taxa in a single grid-cell representing an assemblage to include.",
      "type": "table",
      "constraints": {
        "selection": "single",
        "content": [
          []
        ]
      }
    },
    "distance": {
      "description": "Use distance",
      "type": "boolean",
      "constraints": {
        "default": false
      }
    },
    "weighting": {
      "description": "Weighting",
      "type": "list",
      "constraints": {
        "selection": "single",
        "content": [
          "equally",
          "by number of species"
        ]
      }
    },
    "subsample": {
      "description": "Do sub sample",
      "type": "boolean",
      "constraints": {
        "default": false
      }
    },
    "sitePairsSize": {
      "description": "Site pairs size.",
      "type": "int",
      "constraints": {
        "min": -1,
        "max": -1,
        "default": 1
      }
    }
  },
  "output": {
    "layer": {
      "description": "Output layers."
    },
    "metadata": {
      "description": "GDM metadata."
    },
    "file": {
      "description": "Other output files."
    }
  }
}