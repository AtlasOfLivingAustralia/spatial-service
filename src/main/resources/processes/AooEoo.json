{
  "name": "AooEoo",
  "description": "Area of Occupancy, Extent of Occurrence and Alpha Hull",
  "isBackground": false,
  "version": 1.0,
  "private": {
    "unique": false,
    "isPublic": true
  },
  "input": {
    "biocacheServiceUrl": {
      "description": "biocacheServiceUrl",
      "type": "auto"
    },
    "resolution": {
      "description": "Grid resolution",
      "type": "double",
      "constraints": {
        "min": 0.001,
        "max": 1,
        "default": 0.02
      }
    },
    "area": {
      "description": "Area to restrict the species.",
      "type": "area",
      "constraints": {
        "min": 1,
        "max": 1,
        "minArea": -1,
        "maxArea": -1
      }
    },
    "species": {
      "description": "Occurrences to use.",
      "type": "species",
      "constraints": {
        "min": 1,
        "max": 1
      }
    },
    "coverage": {
      "description": "Alpha value for alpha hull",
      "type": "double",
      "constraints": {
        "min": 0.01,
        "default": 2
      }
    },
    "radius": {
      "description": "Radius (m) for points with radius",
      "type": "double",
      "constraints": {
        "min": 1,
        "max": 1000000,
        "default": 5000
      }
    }
  },
  "output": {
    "metadata": {
      "description": "Output summary."
    },
    "areas": {
      "description": "Output areas."
    },
    "files": {
      "description": "Output files."
    },
    "download": {
      "description": "Files in the download zip."
    }
  }
}
