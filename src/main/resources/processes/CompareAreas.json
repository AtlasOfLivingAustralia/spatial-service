{
  "name": "CompareAreas",
  "description": "Compare areas.",
  "isBackground": false,
  "version": 1.0,
  "private": {
    "unique": false,
    "public": true
  },
  "input": {
    "biocacheServiceUrl": {
      "description": "biocacheServiceUrl",
      "type": "auto"
    },
    "area": {
      "description": "Areas for comparison.",
      "type": "area",
      "constraints": {
        "min": 2
      }
    },
    "species": {
      "description": "Restrict species to use.",
      "type": "species",
      "constraints": {
        "min": 1,
        "max": 1
      }
    }
  },
  "output": {
    "files": {
      "description": "Output files."
    },
    "download": {
      "description": "Files in the download zip."
    },
    "metadata": {
      "description": "Compare areas metadata."
    }
  }
}