{
  "name": "GDMStep1",
  "description": "GDM Step 1.",
  "isBackground": false,
  "version": 1.0,
  "private": {
    "unique": false,
    "public": true
  },
  "input": {
    "biocacheServiceUrl": {
      "description": "biocacheServiceUrl",
      "type": "auto"
    },
    "layersServiceUrl": {
      "description": "layersServiceUrl",
      "type": "auto"
    },
    "area": {
      "description": "Area to restrict the GDM.",
      "type": "area",
      "constraints": {
        "min": 1,
        "max": 1
      }
    },
    "layer": {
      "description": "Layers to use in the GDM. 1 to 10.",
      "type": "layer",
      "constraints": {
        "min": 1,
        "max": 10,
        "contextual": true,
        "environmental": true,
        "analysis": true
      }
    },
    "species": {
      "description": "Occurrences to use in the GDM.",
      "type": "species",
      "constraints": {
        "min": 1,
        "max": 1
      }
    },
    "resolution": {
      "description": "The layer resolution to use.",
      "type": "double",
      "constraints": {
        "min": 0.01,
        "max": 1.0,
        "default": 0.01
      }
    }
  },
  "output": {
    "nextprocess": {
      "description": "This is GDMStep2, if this is run successfully."
    }
  }
}