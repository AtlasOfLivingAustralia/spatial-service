{
  "name": "MergeAreas",
  "description": "Merge areas",
  "isBackground": false,
  "version": 1.0,
  "private": {
    "unique": false,
    "isPublic": true
  },
  "input": {
    "area": {
      "description": "Areas to merge.",
      "type": "area",
      "constraints": {
        "min": 1,
        "max": 100,
        "minArea": -1,
        "maxArea": -1
      }
    },
    "name": {
      "description": "Area name",
      "type": "string"
    },
    "description": {
      "description": "Area description",
      "type": "text",
      "constraints": {
        "optional": false
      }
    },
    "type": {
      "description": "Merge type",
      "type": "list",
      "constraints": {
        "selection": "single",
        "content": [
          "Union",
          "Intersection"
        ],
        "defaultValue": "Union"
      }
    }
  },
  "output": {
    "areas": {
      "description": "Merged output area."
    }
  }
}
