{
  "name": "ScatterplotList",
  "description": "List of scatterplots.",
  "isBackground": false,
  "version": 1.0,
  "private": {
    "unique": false,
    "public": true
  },
  "input": {
    "biocacheServiceUrl": {
      "description": "biocacheServiceUrl",
      "type": "auto"
    },
    "area": {
      "description": "Area to restrict the scatterplot.",
      "type": "area",
      "constraints": {
        "min": 1,
        "max": 1
      }
    },
    "species1": {
      "description": "Foreground species.",
      "type": "species",
      "constraints": {
        "min": 1,
        "max": 1
      }
    },
    "species2": {
      "description": "Background species.",
      "type": "species",
      "constraints": {
        "optional": true,
        "min": 0,
        "max": 1
      }
    },
    "layer": {
      "description": "Layers to use in the scatterplot. Between 3 and 10 layers.",
      "type": "layer",
      "constraints": {
        "min": 3,
        "max": 10,
        "contextual": false,
        "environmental": true,
        "analysis": true
      }
    },
    "resolution": {
      "description": "The layer resolution to use.",
      "type": "list",
      "constraints": {
        "selection": "single",
        "default" : 0.01,
        "content": [
          "0.01",
          "0.1",
          "0.5",
          "1.0"
        ]
      }
    }
  },
  "output": {
    "metadata": {
      "description": "ScatterplotList metadata."
    },
    "download": {
      "description": "Files in the download zip."
    }
  }
}