{
  "name": "PhylogeneticDiversity",
  "description": "Phylogenetic diversity.",
  "isBackground": false,
  "version": 1.0,
  "private": {
    "unique": false,
    "public": true
  },
  "input": {
    "biocacheServiceUrl": {
      "description": "biocacheServiceUrl",
      "type": "auto"
    },
    "phyloServiceUrl": {
      "description": "phyloServiceUrl",
      "type": "auto"
    },
    "area": {
      "description": "Areas to generate phylogenetic diversity.",
      "type": "area",
      "constraints": {
        "min": 1
      }
    },
    "species": {
      "description": "Restrict species to use.",
      "type": "species",
      "constraints": {
        "min": 1,
        "max": 1
      }
    },
    "phylo": {
      "description": "Phylogenetic trees to use.",
      "type": "phylogeneticTree",
      "constraints": {
        "min": 1
      }
    }
  },
  "output": {
    "files": {
      "description": "Output files."
    },
    "download": {
      "description": "Files in the download zip."
    }
  }
}